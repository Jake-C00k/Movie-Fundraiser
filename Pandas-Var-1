#include <iostream>
int indent = 2;
using namespace std;
int pandas(string x, string y, string z, string a)
{
    string list[4] = {x, y, z, a};
    for(int i = 0; i < 4; i++)
    {
        cout << list[i] << "    "; 
        if(i == indent - 1)
        {
            cout << endl;
        }
    }
    return 0;
}

int main()
{
    //Example Implementation
    string alpha = "Listy";
    string beta = "price";
    string gamma = "secondlisty";
    string delta = "secondprice";
    pandas(alpha, beta, gamma, delta);

    return 0;
}
