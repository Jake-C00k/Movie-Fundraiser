#include <iostream>

using namespace std;
int pandas(string x, string y, string z, string a, int d)
{
    string list[4] = {x, y, z, a};
    for(int i = 0; i < 4; i++)
    {
        cout << list[i] << "    "; 
        if(i == d - 1)
        {
            cout << endl;
        }
    }
    return 0;
}

int main()
{
    //example implementation
    string alpha = "Listy";
    string beta = "price";
    string gamma = "secondlisty";
    string delta = "secondprice";
    int indent = 2;
    pandas(alpha, beta, gamma, delta, indent);

    return 0;
}
